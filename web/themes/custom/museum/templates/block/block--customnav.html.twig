{%
set classes = [
'block-content',
'block-content--' ~ bundle|clean_class,
'block-content--' ~ id|clean_class,
bundle|clean_class
]
%}
<div{{ attributes.addClass(classes)|without('id') }}>
  <div class="navlogo">
    <img src="/themes/custom/museum/assets/images/pantheon.png">
  </div>
  {{ title_prefix }}
  {% if label %}
    {{ label }}
  {% endif %}
  {{ title_suffix }}
  {% block content %}
    {{ content }}
  {% endblock %}
</div>
